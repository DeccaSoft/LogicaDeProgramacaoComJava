package classes;

public class Produto {
	String nome;
	Double valor;
	Integer quantidade;
	
	public void exibirProdutoInterno(Produto p){
		System.out.println("Exibindo de Dentro da Classe Produto...");
		System.out.println("O Produto " + p.nome + " custa R$ " + p.valor + " e tem " + p.quantidade + " unidade(s) em estoque.");
	}
}
